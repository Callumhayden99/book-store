generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model Book {
  id          Int      @id @default(autoincrement())
  name        String
  genre       String?
  price       Float?
  author      String?
  numPages    Int?
  weight      Float?
  description String?
  image       String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @default(now())
}
